}

.tb-worksheet-section-text {
    color: #008080 !important;
}

.tb-worksheet-section-currency {
    color: #00ffff !important;
    text-align: right !important;
}

.tb-worksheet-category-text {
    color: #008000 !important;
}

.tb-worksheet-category-currency {
    color: #00cc00 !important;
    text-align: right !important;
}

.tb-worksheet-account-text {
    color: #c0c080 !important;
}

.tb-worksheet-account-currency {
    color: #ffff00 !important;
    text-align: right !important;
}

