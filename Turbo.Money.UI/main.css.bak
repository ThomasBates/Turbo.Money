        display: initial;
    }
}
/* hi-res laptops and desktops */
@media (hover:none ) {
    .test7 {
        display: initial;
    }
}
/* Does not support hover */
@media (hover:hover ) {
    .test8 {
        display: initial;
    }
}
/* Supports hover */



@media screen and (hover:none) /*and (pointer:none)*/ {
    .tb-header-text-mobile {
        display: initial;
    }
    .tb-header-text-desktop {
        display: none;
    }
}
