body {
}

* {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

.add-test-container {
    /*height: 100vh;*/
    font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.list {
    text-align: left;
    max-width: 750px;
    margin: auto;
}

.submit-form {
    max-width: 300px;
    margin: auto;
}

.tb-header {
    background-color: #040;
    text-align: center;
    padding: 20px;
}

.tb-header-text-mobile {
    display: none;
    color: red;
    background-color: white;
}

.tb-header-text-desktop {
    color: white;
    background-color: red;
}

/* The navbar container */
.tb-navbar {
    overflow: hidden;
    background-color: #333;
}

/* Navbar links */
.tb-navbar a {
    float: left;
    display: block;
    color: #f2f2f2;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
}

/* Links - change color on hover */
.tb-navbar a:hover {
    background-color: #ddd;
    color: black;
}

/* Create three equal columns that float next to each other */
.tb-app-column {
    float: left;
    width: 33.33%;
}

/* Clear floats after the columns */
.tb-app-row {
    display: flex;
    flex-wrap: wrap;
}

.tb-sidebar {
    flex: 20%;
    color: #880;
    background-color: #400;
    padding: 20px;
}

.tb-main {
    flex: 80%;
    background-color: #004;
    padding: 20px;
}

.tb-footer {
    padding: 20px;
    text-align: center;
    background: #040;
}

.tb-worksheet-button-left {
    height: 25px;
    padding: 0px 2px;
    margin: 0px 2px;
    opacity: 0.2;
    float: left;
}

.tb-worksheet-button-right {
    height: 25px;
    padding: 0px 2px;
    margin: 0px 2px;
    opacity: 0.2;
    float: right;
}

tr:hover .tb-worksheet-button-left {
    opacity: 1;
}

tr:hover .tb-worksheet-button-right {
    opacity: 1;
}

.tb-worksheet-buttons::after {
    content: "";
    clear: both;
    display: table;
}

.tb-worksheet-tooltip {
    position: absolute;
    background-color: rgba(0, 0, 0, 0.85);
    padding: 2px 10px;
    color: white;
    border-radius: 3px;
}
