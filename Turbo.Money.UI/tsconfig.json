    "baseUrl": "src",
    "noImplicitAny": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true,
    "paths": {
      "src/*":  ["src/*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
  //"include": [
